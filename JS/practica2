function calcularMedia() {
  let media = 0;
  let j = 0;

  for (let i = 0; i < numeros.length; i++) {
    media += numeros[i];
    j++;
  }
  return console.log(media / j);
}
var numeros = [16, 24, 42];
calcularMedia(numeros);

var suma = [];
for (let i = 0; i < suma.length; i++) {
  if (i % 2 == 0) {
    i += suma[i];
  }
  if (i > 99) {
    break;
  }
}
console.log(suma[99]);
